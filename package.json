{
    "name": "shopify-bayashi",
    "version": "0.1.0",
    "private": false,
    "main": "index.js",
    "dependencies": {
        "@react-icons/all-files": "^4.1.0",
        "p5": "^1.3.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-icons": "^3.10.0",
        "react-p5-wrapper": "^2.4.0",
        "react-redux": "^7.2.0",
        "react-router-dom": "^5.2.0",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "shopify-buy": "^2.11.0",
        "video-react": "^0.14.1",
        "yarn": "^1.22.10"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^4.7.0",
        "eslint": "^6.6.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.5",
        "husky": "^4.3.0",
        "prettier": "^2.1.2",
        "pretty-quick": "^3.1.0",
        "react-scripts": "3.4.1"
    },
    "scripts": {
        "heroku-postbuild": "npm run build",
        "dev": "react-scripts start",
        "start": "node index.js",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "lint": "eslint --ext .js,.jsx,.json",
        "lint:fix": "yarn lint --fix"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged",
            "pre-push": "yarn run lint"
        },
        "eslintIgnore": [
            "node_modules",
            "build/**/*.js",
            "dist/**/*.js"
        ]
    }
}
